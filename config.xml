<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lbt.www" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LBT</name>
    <description>
        Local blind test
    </description>
    <author email="hrynchuk.orest@gmail.com" href="https://github.com/OrestHk/">
        OrestHk
    </author>
    <author email="mathieu.lauffenburger.dev@gmail.com" href="https://github.com/Madarow">
        Madarow
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <plugin name="cordova-plugin-file" spec="1" />
    <plugin name="cordova-plugin-media" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
</widget>
